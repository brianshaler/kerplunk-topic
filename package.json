{
  "name": "kerplunk-topic",
  "displayName": "Topics",
  "description": "Adds a Topic field to activity items and performs basic tokenization on activity items",
  "version": "0.0.3",
  "kerplunk": {
    "permissions": {
      "kerplunk-activityitem": {
        "models": [
          "ActivityItem"
        ]
      }
    }
  },
  "keywords": [
    "kerplunk-plugin",
    "kp:Data Type"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "WNdb": "^3.1.1",
    "lodash": "^3.7.0",
    "natural": "^0.2.1",
    "react": "^0.13.2",
    "stopwords": "0.0.4",
    "when": "^3.7.3"
  },
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "glut": "0.0.1",
    "glut-coffee-amdify": "0.0.0",
    "gulp": "^3.8.8",
    "gulp-coffee": "^2.3.1",
    "gulp-stylus": "^2.0.1"
  },
  "scripts": {
    "prepublish": "rm -rf ./lib ./public && gulp"
  }
}
